{
  "name": "@aokiapp/rjsf-mantine-theme",
  "version": "0.1.1",
  "main": "./dist/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "description": "Mantine theme, fields and widgets for react-jsonschema-form",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "cs-check": "prettier -l \"{src,test}/**/*.ts?(x)\"",
    "cs-format": "prettier \"{src,test}/**/*.ts?(x)\" --write",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "test": "jest",
    "test:update": "jest --u"
  },
  "lint-staged": {
    "{src,test}/**/*.ts?(x)": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "@mantine/core": ">=7.4.0",
    "@mantine/dates": ">=7.4.0",
    "@mantine/hooks": ">=7.4.0",
    "@rjsf/core": ">=5.16.1",
    "@rjsf/utils": ">=5.16.1",
    "@tabler/icons-react": ">=2.45.0",
    "dayjs": ">=1.11.10",
    "react": ">=17"
  },
  "devDependencies": {
    "@rjsf/snapshot-tests": "^5.16.x",
    "@types/react-test-renderer": "^18.0.7",
    "react-test-renderer": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Yuki Aoki<me@aoki.app>",
  "contributors": [],
  "keywords": [
    "react-jsonschema-form",
